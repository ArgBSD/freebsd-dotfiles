! set urxvt to xterm-256color
!URxvt*termName: xterm-256color

! font settings
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! urxvt
URxvt.letterSpace: -1
URxvt.internalBorder: 4
URxvt*scrollstyle: plain
URxvt*scrollBar: False
URxvt*modifier: mod1
URxvt*urgentOnBell: true
URxvt*visualBell:   false
URxvt*font:           xft:Inconsolata:size=14,xft:DejaVu Sans Mono:size=11
URxvt*perl-ext-common: default,clipboard,selection-to-clipboard,font-size
! copy and paste
URxvt.keysym.Shift-C-C: eval:selection_to_clipboard
URxvt.keysym.Shift-C-V: eval:paste_clipboard
! Turn on/off ISO 14755 (default enabled).
URxvt*iso14755: False
URxvt.iso14755_52: false

! emacs
Emacs*toolBar: 0
Emacs.verticalScrollBars: off
emacs.font: Inconsolata:size=18:weight=regular:antialias=true:hinting=true:hintstyle=hintfull

! color
*background: #002b36
*foreground: #839496
!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #449307
*color10: #56B30B

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3
